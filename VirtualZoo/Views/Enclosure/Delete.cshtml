@model VirtualZooShared.Models.Enclosure

<h2>Delete Enclosure</h2>

<p>Are you sure you want to delete this enclosure?</p>
<div>
    <strong>Name:</strong> @Model.Name
</div>
<div>
    <strong>Size:</strong> @Model.Size
</div>
<div>
    <strong>Security Level:</strong> @Model.SecurityLevel
</div>

<form asp-action="DeleteConfirmed" method="post">
    <input type="hidden" name="id" value="@Model.Id" />
    <button type="submit" class="btn btn-danger">Delete</button>
    <a href="@Url.Action("Index", "Enclosure")" class="btn btn-secondary">Cancel</a>
</form>
